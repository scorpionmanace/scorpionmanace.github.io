{
  "name": "scorpionmanace.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run dev",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.0",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.{ts,tsx}"
        ],
        "parser": "@typescript-eslint/parser",
        "rules": {
          "@typescript-eslint/no-unused-vars": "warn"
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
